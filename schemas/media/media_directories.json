{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "directories": {
      "type": "array",
      "items": [
        {
          "type": "object",
          "properties": {
            "key": {
              "type": "string"
            },
            "path": {
              "type": "string"
            }
          },
          "required": ["key", "path"]
        }
      ]
    }
  },
  "required": ["directories"]
}
